<script>
	function addParamToPageTag() {
		/**resultlistForm为翻页组件ID**/
		var start = $('#resultlistForm input[name="resultPagination.start"]').val();
		// 第一次进入时才加载
		if (start == 0) {
			// 可执行检索式
			var exe = $('#resultlistForm input[name="searchCondition.executableSearchExp"]').val();
			if (exe == undefined || exe == '' || exe == null) {
				$('#resultlistForm').append('<input type="hidden" name="searchCondition.executableSearchExp" value="'+ $('#result_executableSearchExp').text() + '"/>');
				exe = $('#resultlistForm input[name="searchCondition.executableSearchExp"]').val();
			}
			
			var drillSearchFlag = $('#drillSearchFlag');
			if(drillSearchFlag == '' || drillSearchFlag == null || drillSearchFlag == undefined || drillSearchFlag == 'undefined'){
				// 检索式id
				var ssId = $('#resultlistForm input[name="searchCondition.executableSearchExp"]').val();
				if (ssId == undefined || ssId == '' || ssId == null) {
					$('#resultlistForm').append('<input type="hidden" name="searchCondition.ssId" value="'+ $('#result_ssId').val() + '"/>');
				}
			}
			// dbId
			var dbId = $('#resultlistForm input[name="searchCondition.dbId"]').val();
			if (dbId == undefined) {
				$('#resultlistForm').append('<input type="hidden" name="searchCondition.dbId" value=""/>');
			}
			// 判断可执行检索式中是否有dbid
			if (exe.indexOf('VDB:') == -1) {
				$('#resultlistForm input[name="searchCondition.dbId"]').val('VDB');
			} else {
				$('#resultlistForm input[name="searchCondition.dbId"]').val('');
			}
			// 检索文献库用检索式
			var literatureSF = $('#resultlistForm input[name="searchCondition.literatureSF"]').val();
			if (literatureSF == undefined) {
				$('#resultlistForm').append("<input type=\"hidden\" name=\"searchCondition.literatureSF\" value=\""+ $('#result_literatureSF').text().replace(/"/gm,"&quot;") + "\"/>");
			}
			// 检索文献库用检索策略
			var strategy = $('#resultlistForm input[name="searchCondition.strategy"]').val();
			if (strategy == undefined) {
				$('#resultlistForm').append('<input type="hidden" name="searchCondition.strategy" value="'+ $('#result_strategy').val() + '"/>');
			}
		}
		// 检索因子--每次都重新传，因为分页标签不支持传数组
		var keys = $('#resultlistForm input[name="searchCondition.searchKeywords"]').val();
		var keywords = dealSearchKeywords;
		if (keys == undefined) {
			if (keywords.length > 0 && keywords[0] != '') {
				keywords = new Array("").concat(keywords);
			}
		} else {
			$('#resultlistForm input[name="searchCondition.searchKeywords"]') .remove();
		}
		$.each(keywords, function(i, value) {
			$('#resultlistForm').append('<input type="hidden" name="searchCondition.searchKeywords" value="' + value + '"/>');
		});
	}
	addParamToPageTag();
</script>



